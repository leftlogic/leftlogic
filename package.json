{
  "name": "leftlogic",
  "version": "3.0.0",
  "description": "leftlogic.com website",
  "private": true,
  "scripts": {
    "dev": "next",
    "build": "next build && next export",
    "build-css": "node-sass-chokidar ./styles/ -o ./static/css/",
    "watch":
      "npm run build-css && node-sass-chokidar ./styles/ -o ./static/css/ --watch --recursive",
    "start": "next start"
  },
  "engines": {
    "node": "8.x.x"
  },
  "now": {
    "alias": "leftlogic.com"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/leftlogic/leftlogic.git"
  },
  "author": "Remy Sharp",
  "license": "MIT",
  "readmeFilename": "README.md",
  "dependencies": {
    "next": "^4.1.4",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "react-ga": "^2.3.5",
    "react-remarkable": "^1.1.3"
  },
  "devDependencies": {
    "@remy/eslint": "^1.1.1",
    "babel-eslint": "^8.0.2",
    "eslint": "^4.12.0",
    "eslint-config-react-app": "^2.0.1",
    "eslint-plugin-flowtype": "^2.39.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.5.1",
    "node-sass": "^4.7.2",
    "node-sass-chokidar": "0.0.3"
  }
}
