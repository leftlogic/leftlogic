{
  "name": "leftlogic",
  "version": "3.0.0",
  "description": "leftlogic.com website",
  "private": true,
  "scripts": {
    "dev": "next",
    "build": "npm run build-css && next build && next export",
    "build-css": "node-sass-chokidar ./styles/ -o ./static/css/",
    "watch": "npm run build-css && node-sass-chokidar ./styles/ -o ./static/css/ --watch --recursive",
    "start": "next start"
  },
  "engines": {
    "node": "10.x.x"
  },
  "now": {
    "alias": "leftlogic.com"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/leftlogic/leftlogic.git"
  },
  "author": "Remy Sharp",
  "license": "MIT",
  "readmeFilename": "README.md",
  "dependencies": {
    "next": "^13.0.2",
    "react": "^16.8.4",
    "react-dom": "^16.8.4"
  },
  "devDependencies": {
    "@mdx-js/loader": "^1.0.21",
    "@next/mdx": "^8.1.0",
    "@remy/eslint": "^1.3.0",
    "node-sass-chokidar": "1.3.4"
  },
  "//": {
    "node-sass": "^4.7.2",
    "react-ga": "^2.4.1",
    "react-remarkable": "^1.1.3",
    "babel-eslint": "^8.2.2",
    "eslint": "^4.18.1",
    "eslint-config-react-app": "^2.1.0",
    "eslint-plugin-flowtype": "^2.46.1",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0"
  }
}
